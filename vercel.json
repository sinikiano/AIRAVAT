{
  "version": 2,
  "name": "airavat-control-panel",
  "builds": [
    {
      "src": "WEB PANEL/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/WEB PANEL/index.html"
    },
    {
      "src": "/index.html",
      "dest": "/WEB PANEL/index.html"
    },
    {
      "src": "/script-supabase.js",
      "dest": "/WEB PANEL/script-supabase.js"
    },
    {
      "src": "/supabase-client.js",
      "dest": "/WEB PANEL/supabase-client.js"
    },
    {
      "src": "/jquery.js",
      "dest": "/WEB PANEL/jquery.js"
    },
    {
      "src": "/img/(.*)",
      "dest": "/WEB PANEL/img/$1"
    },
    {
      "src": "/imgg/(.*)",
      "dest": "/WEB PANEL/imgg/$1"
    },
    {
      "src": "/asset/(.*)",
      "dest": "/WEB PANEL/asset/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_ANON_KEY": "@supabase_anon_key"
  }
}
